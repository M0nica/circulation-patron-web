{
  "name": "simplified-circulation-patron-web",
  "version": "0.0.1",
  "description": "patron UI for Circulation Manager",
  "repository": {
    "type": "git",
    "url": "https://github.com/NYPL-Simplified/circulation-patron-web.git"
  },
  "author": "NYPL",
  "license": "Apache-2.0",
  "scripts": {
    "prepublish": "typings install && npm run prod",
    "lint": "tslint -c tslint.json src/*.ts src/*.tsx src/**/*.ts src/**/*.tsx src/**/**/*.ts src/**/**/*.tsx",
    "test": "npm run lint && tsc && mocha --require lib/testHelper.js lib/**/__tests__/*.js",
    "dev": "rm -rf dist && NODE_ENV=development webpack --watch --progress --display-modules --config webpack.config",
    "prod": "rm -rf dist && NODE_ENV=production webpack -p --progress --display-modules --config webpack.config",
    "start": "tsc && node lib/server/index.js"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "file-loader": "^0.9.0",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.2.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "express": "^4.14.0",
    "opds-web-client": "0.0.20",
    "react": "^15.2.0",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
